
<h3>Login</h3>
<form method="post" asp-action="login" asp-controller="account">
	<table>
	<tr>
		<td>Username</td>
		<td>
			<input type="text"  />
		</td>
	</tr>
	<tr>
		<td>Password</td>
		<td>
			<input type="password"  />
		</td>
	</tr>
	<tr>
		<td>&nbsp;</td>
		<td>
			<input type="submit" value="Login" />
		</td>
	</tr>
	
</table>
</form>

